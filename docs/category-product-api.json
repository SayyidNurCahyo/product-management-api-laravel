            "get": {
                "description": "API untuk mengambil data kategori produk dengan menggunakan metode token / JWT",
                "parameters": [
                    {
                        "name": "Authorization",
                        "in": "header"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Berhasil mendapatkan data kategori produk",
